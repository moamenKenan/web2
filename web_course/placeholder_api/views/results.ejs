<% include partial/header %>

<h1>the results : </h1>
<div class="person"> 
        <table class="bg-primary">
                <tr>
                        <th> id</th>
                        <th>name</th>
                        <th>username</th>
                        <th>email</th>
                        <th>city</th>
                        <th>street</th>

                    </tr>
<% persons.forEach(function(element){ %>
            <tr>
                <td>
                        <%= element["id"] %>
                </td>
                <td>
                    <%= element["name"] %>
                </td>
                <td>
                        <%= element["username"] %>
                </td>
                <td>
                        <%= element["email"] %>
                </td>
                <td>
                        <%= element["address"]["city"] %>
                </td>
                <td>
                        <%= element["address"]["street"] %>                 
                </td>
            </tr>
<%});%>
</table>
</div>

<% include partial/footer %>